    "version": "0.2.0",
    "configurations": [
        {
            "name": "Parse RocketTile VCD",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/src/main.py",
            "args": [
                "--vcd",
                "${workspaceFolder}/RocketTile_Small/1561.vcd",
                "--inputs",
                "${workspaceFolder}/RocketTile_Small/input.list",
                "--output",
                "${workspaceFolder}/RocketTile_Small/output.csv",
            ],
            "console": "integratedTerminal",
            "justMyCode": true,
            "env": {
                "PYTHONPATH": "${workspaceFolder}/src"
            },
            "cwd": "${workspaceFolder}"
        }
    ]
}